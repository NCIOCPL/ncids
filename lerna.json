{
  "command": {
    "publish": {
      "registry": "https://npm.pkg.github.com",
      "access": "public"
    }
  },
  "packages": [
    "packages/*",
    "testing/*",
    "docs"
  ],
  "publish": {
    "allowBranch": [
      "develop",
      "ticket/*",
      "hotfix/*",
      "main"
    ]
  },
  "npmClient": "pnpm",
  "version": "3.1.0",
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
