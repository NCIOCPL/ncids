{
	"command": {
		"publish": {
			"registry": "https://npm.pkg.github.com",
			"access": "public"
		}
	},
	"packages": [
		"packages/*",
		"testing/*",
		"docs"
	],
	"publish": {
		"allowBranch": [
			"develop",
			"ticket/*",
			"hotfix/*",
			"main"
		]
	},
	"npmClient": "yarn",
	"useWorkspaces": true,
	"version": "2.0.0-alpha.1"
}
