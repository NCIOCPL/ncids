{
  "command": {
    "publish": {
      "registry": "https://npm.pkg.github.com",
      "access": "public"
    }
  },
  "packages": [
    "packages/*",
    "testing/*",
    "docs"
  ],
  "publish": {
    "allowBranch": [
      "develop",
      "ticket/*",
      "hotfix/*",
      "main"
    ]
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "2.5.0",
  "useNx": false
}
